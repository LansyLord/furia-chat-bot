{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/angular",
            "config": {
                "dist": "dist/furia-chat-bot-front/browser"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}