{
    "version": 2,
    "builds": [
        {
            "src": "furia-chat-bot-front/angular.json",
            "use": "@vercel/builders",
            "config": {
                "dist": "furia-chat-bot-front/dist/furia-chat-bot-front/browser"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}