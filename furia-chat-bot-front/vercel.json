{
    "version": 2,
    "builds": [
        {
            "src": "dist/furia-chat-bot-front/server/main.js",
            "use": "@vercel/node"
        },
        {
            "src": "dist/furia-chat-bot-front/browser/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server/main.js"
        }
    ]
}