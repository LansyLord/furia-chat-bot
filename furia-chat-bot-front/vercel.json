{
    "version": 2,
    "builds": [
        {
            "src": "dist/furia-chat-bot-front/server/main.server.mjs",
            "use": "@vercel/node",
            "config": {
                "nodeVersion": "20.x",
                "runtime": "nodejs20.x",
                "includeFiles": [
                    "dist/furia-chat-bot-front/browser/**",
                    "dist/furia-chat-bot-front/server/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/furia-chat-bot-front/server/main.server.mjs"
        }
    ]
}